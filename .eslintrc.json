{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "no-console": "off",
	    "indent": ["error", 4],
	    "max-classes-per-file": ["error", 4],
        "prefer-const" : "off"  
    }
}
